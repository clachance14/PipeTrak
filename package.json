{
  "private": true,
  "scripts": {
    "build": "dotenv -c -- turbo build",
    "dev": "dotenv -c -- turbo dev --concurrency 25",
    "db:push": "dotenv -c -- turbo run db:push",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.8",
    "turbo": "^1.10.7",
    "typescript": "5.1.3"
  },
  "packageManager": "pnpm@8.3.0",
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "prettier-plugin-tailwindcss": "^0.3.0"
  }
}
