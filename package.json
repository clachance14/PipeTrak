{
  "private": true,
  "scripts": {
    "build": "pnpm prisma:generate && dotenv -c -- turbo build",
    "dev": "pnpm prisma:generate && dotenv -c -- turbo dev --concurrency 25",
    "prisma:generate": "prisma generate",
    "prisma:push": "dotenv -c -- prisma db push --skip-generate",
    "prisma:studio": "dotenv -c -- prisma studio",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.8",
    "turbo": "^1.10.7",
    "typescript": "5.1.3"
  },
  "packageManager": "pnpm@8.3.0",
  "devDependencies": {
    "dotenv-cli": "^7.2.1",
    "prisma": "^5.0.0",
    "prettier-plugin-tailwindcss": "^0.3.0"
  }
}
