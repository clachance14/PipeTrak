datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["jsonProtocol"]
}

model Subscription {
  id              String    @id @default(cuid())
  userId          String
  customerId      String
  subscriptionId  String
  status          String
  planId          String
  variantId       String
  nextPaymentDate DateTime?
}
